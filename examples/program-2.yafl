program
  integer i, j, k, l;
start
  input j;
  k := 2**j;
  output k;

  i := !(2+4 >= 1) || !!(4 > 1);
  output i;

  i := !!!(!2+4);
  output i;

  i := 4;
  l := 6;

  i := i ^ l;
  l := l ^ i;
  i := i ^ l;

  output i;
  output l;

  output 2 | 4 & 12;
  output 4 << 4;
  output 4^4;

  output !(3**2);

  i := 1;

  while i = 16 {
    if (i >= 5) {
      output i;
    }
    else {
      skip;
    };

    i := i+1;
  };
finish
